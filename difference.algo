ALGORITHM difference
VAR
set1: ARRAY_OF INTEGER[] := {3, 1, 7, 9};
set2: ARRAY_OF INTEGER[] :={2, 4, 1, 9, 3};
sum, i, j: INTEGER;
    
BEGIN
sum:= 0
  FOR i FROM 0 TO set1.length STEP 1  DO
    FOR j FROM 0 TO set2.length STEP step  DO
    IF (set1[i]=set2[j]) THEN
        sum:=sum +set1[i]
    END_IF
        
    END_FOR
  END_FOR
END